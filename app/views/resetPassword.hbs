        <section class="Form my-4 mx-5">
            <div class="container">
                <div class="row cartRow">
                    <div class="col-lg-7 px-5 pt-5">
                        <h1 class="font-weight-bold py-3">New password</h1>
                        <form action="">
                            <div class="form-row">
                                <div class="col-lg-7">
                                    <input type="password" class="form-control my-3
                                        p-4 newPass" placeholder="NewPassword" />
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-lg-7">
                                    <input type="password" class="form-control
                                        my-3 p-4 confirmPass"
                                        placeholder="Confirm Password" />
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-lg-7 mainLoad">
                                    <button type="submit" class="btn1 mt-3 mb-5
                                        success">Confirm</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </section>






